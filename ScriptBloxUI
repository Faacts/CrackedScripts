		SCRIPT_Execute.TextSize = 30.000
		SCRIPT_Execute.TextTransparency = 1.000
		SCRIPT_Corner_1.Name = "SCRIPT_Corner_1"
		SCRIPT_Corner_1.Parent = SCRIPT_Execute
		SCRIPT_Copy.Name = "SCRIPT_Copy"
		SCRIPT_Copy.Parent = SCRIPT_Example
		SCRIPT_Copy.AnchorPoint = Vector2.new(0.5, 0.5)
		SCRIPT_Copy.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		SCRIPT_Copy.BackgroundTransparency = 1.000
		SCRIPT_Copy.Position = UDim2.new(0.5, 0, 0.75, 0)
		SCRIPT_Copy.Size = UDim2.new(0, 150, 0, 50)
		SCRIPT_Copy.ZIndex = 2
		SCRIPT_Copy.AutoButtonColor = false
		SCRIPT_Copy.Font = Enum.Font.ArialBold
		SCRIPT_Copy.Text = "Copy"
		SCRIPT_Copy.TextColor3 = Color3.fromRGB(140, 125, 255)
		SCRIPT_Copy.TextSize = 30.000
		SCRIPT_Copy.TextTransparency = 1.000
		SCRIPT_Corner_2.Name = "SCRIPT_Corner_2"
		SCRIPT_Corner_2.Parent = SCRIPT_Copy
		if SCRIPT_SCRIPT ~= nil then
			SCRIPT_Execute.MouseButton1Click:Connect(function()
				loadstring(SCRIPT_SCRIPT)()
			end)
			SCRIPT_Copy.MouseButton1Click:Connect(function()
				setclipboard(tostring(SCRIPT_SCRIPT))
				setclipboard(tostring(SCRIPT_SCRIPT))
			end)
		end
		SCRIPT_Example.MouseEnter:Connect(function()
			local tween1 = game:GetService("TweenService"):Create(SCRIPT_Copy,TweenInfo.new(.2),{BackgroundTransparency = 0, TextTransparency = 0})
			local tween2 = game:GetService("TweenService"):Create(SCRIPT_Execute,TweenInfo.new(.2),{BackgroundTransparency = 0, TextTransparency = 0})
			local tween3 = game:GetService("TweenService"):Create(SCRIPT_Label_2,TweenInfo.new(.2),{TextTransparency = 0})
			local tween4 = game:GetService("TweenService"):Create(SCRIPT_Label,TweenInfo.new(.1),{TextTransparency = 1})
			local tween5 = game:GetService("TweenService"):Create(SCRIPT_Image,TweenInfo.new(.1),{ImageTransparency = 1, BackgroundTransparency = 1})
			tween1:Play(); tween1:Play(); tween2:Play(); tween3:Play(); tween4:Play(); tween5:Play()
		end)
		SCRIPT_Example.MouseLeave:Connect(function()
			local tween1 = game:GetService("TweenService"):Create(SCRIPT_Copy,TweenInfo.new(.1),{BackgroundTransparency = 1, TextTransparency = 1})
			local tween2 = game:GetService("TweenService"):Create(SCRIPT_Execute,TweenInfo.new(.1),{BackgroundTransparency = 1, TextTransparency = 1})
			local tween3 = game:GetService("TweenService"):Create(SCRIPT_Label_2,TweenInfo.new(.1),{TextTransparency = 1})
			local tween4 = game:GetService("TweenService"):Create(SCRIPT_Label,TweenInfo.new(.2),{TextTransparency = 0})
			local tween5 = game:GetService("TweenService"):Create(SCRIPT_Image,TweenInfo.new(.2),{ImageTransparency = 0, BackgroundTransparency = 0.5})
			tween1:Play(); tween2:Play(); tween3:Play(); tween4:Play(); tween5:Play()
		end)
		SCRIPT_Example.Parent = Home.SEARCHER
	end
end
function search(SCRIPT_search,SCRIPT_page)
	pcall(function()
		PageFrame.PageBox.Text = SCRIPT_page
		local children_B = Home.SEARCHER:GetChildren()
		for k = 1, #children_B do
			if children_B[k]:IsA("Frame") and children_B[k].Name ~= "Page" then
				children_B[k]:Destroy()
			end
		end
		if SCRIPT_search == "START_PAGE"  or string.len(Frame3.Frame.TextBox.Text) <= 2 then
			for _, v in pairs(game:GetService("HttpService"):JSONDecode(game:HttpGet("https://www.scriptblox.com/api/script/fetch?page="..tostring(SCRIPT_page)))) do
				for z,c in pairs(v['scripts']) do
					if not c['isPatched'] then
						if c['game']['name'] == "Universal Script 📌" then
							CreateScriptFrame(c['title'],c['game']['imageUrl'],c['script'], true)
						else
							CreateScriptFrame(c['title'],c['game']['imageUrl'],c['script'], false)
						end
					end
					task.wait(0.01)
				end
			end
		else
			for _, v in pairs(game:GetService("HttpService"):JSONDecode(game:HttpGet("https://scriptblox.com/api/script/search?&mode=free&filters=newest&q="..SCRIPT_search.."&page="..tostring(SCRIPT_page)))) do
				for z,c in pairs(v['scripts']) do
					if not c['isPatched'] then
						if c['game']['name'] == "Universal Script 📌" then
							CreateScriptFrame(c['title'],c['game']['imageUrl'],c['script'], true)
						else
							CreateScriptFrame(c['title'],c['game']['imageUrl'],c['script'], false)
						end
					end
					task.wait(0.01)
				end
			end
		end
	end)
end
Frame.InputBegan:Connect(function(input)
	if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and GuiClosed == false then
		dragToggle = true
		dragStart = input.Position
		startPos = Frame.Position
		input.Changed:Connect(function()
			if (input.UserInputState == Enum.UserInputState.End) then
				dragToggle = false
			end
		end)
	end
end)

Frame.InputChanged:Connect(function(input)
	if (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) and GuiClosed == false then
		dragInput = input
	end
end)

game:GetService("UserInputService").InputChanged:Connect(function(input)
	if (input == dragInput and dragToggle) and GuiClosed == false then
		updateInput(input)
	end
end)
