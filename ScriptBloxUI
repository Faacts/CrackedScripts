	SelectPage("Home")
end)
MainButtons.Upload.MouseButton1Click:Connect(function()
	SelectPage("Upload")
end)
MainButtons.Account.MouseButton1Click:Connect(function()
	SelectPage("Account")
end)
















spawn(function()
	while GuiClosed == false do
		while string.len(SearchBar.PlaceholderText) ~= 0 do
			SearchBar.PlaceholderText = string.sub(SearchBar.PlaceholderText, 1, string.len(SearchBar.PlaceholderText) - 1)
			wait(.015)
		end
		wait(.5)
		local MathRandomText = "Try \""..PlaceholderTexts[math.random(1, #PlaceholderTexts)].."\""
		while string.len(SearchBar.PlaceholderText) ~= string.len(MathRandomText) do
			SearchBar.PlaceholderText = string.sub(MathRandomText, 1, string.len(SearchBar.PlaceholderText) + 1)
			wait(.075)
		end
		wait(5)
	end
end)
SelectPage("Home")
search("START_PAGE",1)
Loadet = Loadet + 1
local tween = game:GetService("TweenService"):Create(SCREENGUI.A_A_LOADING.TextLabel, TweenInfo.new(1), {AnchorPoint = Vector2.new(0.5, 0)}); tween:Play() ; tween.Completed:Wait()
wait(1)
while Loadet ~= 2 do
	wait(.1)
end
SCREENGUI.SCRIPTBLOX.Visible = true
wait(.1)
SCREENGUI.A_A_LOADING.Visible = false
