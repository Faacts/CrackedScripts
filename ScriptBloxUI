function updateInput(input)
	if GuiClosed == false then
		local Delta = input.Position - dragStart
		local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
		game:GetService("TweenService"):Create(Frame, TweenInfo.new(dragSpeed), {Position = Position}):Play()
	end
end
local Abbreviations = {"k", "M", "B", "T", "Qa", "Qn", "Sx", "Sp", "Oc", "N"}
local function formatNumber(Number, Decimals)
	return math.floor(((Number < 1 and Number) or math.floor(Number) / 10 ^ (math.log10(Number) - math.log10(Number) % 3)) * 10 ^ (Decimals or 3)) / 10 ^ (Decimals or 3)..(Abbreviations[math.floor(math.log10(Number) / 3)] or "")
end
function SelectPage(page)
	if GuiClosed == false then
		if page == "Home" then
			NowPage = "Home"
			local TweenInstanceFrame1 = game:GetService("TweenService"):Create(InstanceFrame,TweenInfo.new(dragSpeed*2),{Size = UDim2.new(0.8, 0, 0, 1)}); TweenInstanceFrame1:Play()
			local TweenInstanceFrame2 = game:GetService("TweenService"):Create(Copy1InstanceFrame,TweenInfo.new(dragSpeed),{Size = UDim2.new(0, 0, 0, 1)}); TweenInstanceFrame2:Play()
			local TweenInstanceFrame3 = game:GetService("TweenService"):Create(Copy2InstanceFrame,TweenInfo.new(dragSpeed),{Size = UDim2.new(0, 0, 0, 1)}); TweenInstanceFrame3:Play()
			Home.Visible = true
			Upload.Visible = false
			Account.Visible = false
		elseif page == "Upload" then
			NowPage = "Upload"
			local TweenInstanceFrame2 = game:GetService("TweenService"):Create(Copy1InstanceFrame,TweenInfo.new(dragSpeed*2),{Size = UDim2.new(0.8, 0, 0, 1)}); TweenInstanceFrame2:Play()
			local TweenInstanceFrame2 = game:GetService("TweenService"):Create(Copy2InstanceFrame,TweenInfo.new(dragSpeed),{Size = UDim2.new(0, 0, 0, 1)}); TweenInstanceFrame2:Play()
			local TweenInstanceFrame3 = game:GetService("TweenService"):Create(InstanceFrame,TweenInfo.new(dragSpeed),{Size = UDim2.new(0, 0, 0, 1)}); TweenInstanceFrame3:Play()
			Home.Visible = false
			Upload.Visible = true
			Account.Visible = false
		elseif page == "Account" then
			NowPage = "Account"
			local TweenInstanceFrame3 = game:GetService("TweenService"):Create(Copy2InstanceFrame,TweenInfo.new(dragSpeed*2),{Size = UDim2.new(0.8, 0, 0, 1)}); TweenInstanceFrame3:Play()
			local TweenInstanceFrame2 = game:GetService("TweenService"):Create(InstanceFrame,TweenInfo.new(dragSpeed),{Size = UDim2.new(0, 0, 0, 1)}); TweenInstanceFrame2:Play()
			local TweenInstanceFrame3 = game:GetService("TweenService"):Create(Copy1InstanceFrame,TweenInfo.new(dragSpeed),{Size = UDim2.new(0, 0, 0, 1)}); TweenInstanceFrame3:Play()
			Home.Visible = false
			Upload.Visible = false
			Account.Visible = true
		end
	end
end
function CreateScriptFrame(TEXT_script, IMG_script, SCRIPT_SCRIPT, Universal)
	if GuiClosed == false then
		local SCRIPT_Example = Instance.new("Frame")
		local SCRIPT_Corner_0 = Instance.new("UICorner")
		local SCRIPT_Image = Instance.new("ImageLabel")
		local SCRIPT_Corner_3 = Instance.new("UICorner")
		local SCRIPT_Label = Instance.new("TextLabel")
		local SCRIPT_Label_2 = Instance.new("TextLabel")
		local SCRIPT_Label_3 = Instance.new("TextLabel")
		local SCRIPT_Execute = Instance.new("TextButton")
		local SCRIPT_Corner_1 = Instance.new("UICorner")
		local SCRIPT_Copy = Instance.new("TextButton")
		local SCRIPT_Corner_2 = Instance.new("UICorner")
		SCRIPT_Example.Name = "SCRIPT_Example"
		SCRIPT_Example.BackgroundColor3 = Color3.fromRGB(189, 184, 255)
		SCRIPT_Corner_0.Name = "SCRIPT_Corner_0"
		SCRIPT_Corner_0.Parent = SCRIPT_Example
		SCRIPT_Image.Name = "SCRIPT_Image"
		SCRIPT_Image.Parent = SCRIPT_Example
		SCRIPT_Image.AnchorPoint = Vector2.new(0.5, 0)
		SCRIPT_Image.BackgroundColor3 = Color3.fromRGB(161, 148, 255)
		SCRIPT_Image.BackgroundTransparency = 0.500
		SCRIPT_Image.Position = UDim2.new(0.5, 0, 0.0500000007, 0)
		SCRIPT_Image.Size = UDim2.new(0, 220, 0, 115)
		pcall(function()
			SCRIPT_Image.Image = LoadCustomAsset(IMG_script)
		end)
		SCRIPT_Corner_3.CornerRadius = UDim.new(0, 16)
		SCRIPT_Corner_3.Name = "SCRIPT_Corner_3"
		SCRIPT_Corner_3.Parent = SCRIPT_Image
