		return game:GetService("HttpService"):JSONDecode(Functions.Request({ Url = "https://ptb.discord.com/api/invites/"..code2, Method = "GET" }).Body)
	end)
	local success2, inviteData2 = pcall(function()
		return game:GetService("HttpService"):JSONDecode(Functions.Request({ Url = "https://ptb.discord.com/api/invites/"..code1, Method = "GET" }).Body)
