CloseButton.MouseButton1Click:Connect(function()
	if GuiClosed == false then
		GuiClosed = true
		local Tween1 = game:GetService("TweenService"):Create(Frame, TweenInfo.new(dragSpeed*5), {Position = Frame.Position+UDim2.new(0, 0,2, 0)})
				Tween1:Play(); Tween1.Completed:Wait()
		if isfolder("UniversalScriptBloxSearcher") then
		    delfolder("UniversalScriptBloxSearcher")
		end
		wait(.3)
		SCREENGUI:Destroy()
	end
end)
Frame3.Frame.TextButton.MouseButton1Click:Connect(function()
	page = 1
	search(Frame3.Frame.TextBox.Text,page)
end)
Frame3.Frame.TextBox.FocusLost:Connect(function()
	page = 1
	search(Frame3.Frame.TextBox.Text,page)
end)
PageFrame.PageBox.FocusLost:Connect(function()
	if PageFrame.PageBox.Text ~= " " or PageFrame.PageBox.Text ~= "" then
		if PageFrame.PageBox.Text -1 == PageFrame.PageBox.Text -1 then
			page = tonumber(PageFrame.PageBox.Text)
			search(Frame3.Frame.TextBox.Text,page)
		end
	end
end)
PageFrame[">"].MouseButton1Click:Connect(function()
	if page ~= 0 then
		page = page + 1
		search(Frame3.Frame.TextBox.Text,page)
	else
		PageFrame.PageBox.Text = page
	end
end)
PageFrame["<"].MouseButton1Click:Connect(function()
	if page ~= 100 then
		page = page - 1
		search(Frame3.Frame.TextBox.Text,page)
	elseif page > 100 then
		page = 100
		search(Frame3.Frame.TextBox.Text,page)
	else
		PageFrame.PageBox.Text = page
	end
end)
PageFrame.MouseEnter:Connect(function()
	local tween1 = game:GetService("TweenService"):Create(PageFrame,TweenInfo.new(.15),{Size = UDim2.new(0, 150,0, 50)}); tween1:Play()
	local tween2 = game:GetService("TweenService"):Create(PageFrame[">"],TweenInfo.new(.25),{TextTransparency = 0}); tween2:Play()
	local tween3 = game:GetService("TweenService"):Create(PageFrame["<"],TweenInfo.new(.25),{TextTransparency = 0}); tween3:Play()
end)
PageFrame.MouseLeave:Connect(function()
	local tween1 = game:GetService("TweenService"):Create(PageFrame,TweenInfo.new(.25),{Size = UDim2.new(0, 75,0, 50)}); tween1:Play()
	local tween2 = game:GetService("TweenService"):Create(PageFrame[">"],TweenInfo.new(.15),{TextTransparency = 1}); tween2:Play()
	local tween3 = game:GetService("TweenService"):Create(PageFrame["<"],TweenInfo.new(.15),{TextTransparency = 1}); tween3:Play()
end)
